{
  "name": "chronicler",
  "version": "0.1.0",
  "description": "Blockchain-Based AI Agent Audit System",
  "main": "source/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run source/index.ts",
    "build": "bun build source/index.ts --outdir dist",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "deploy": "bun run scripts/deploy.ts",
    "compile": "bun run scripts/compile.ts",
    "verify": "bun run scripts/verify.ts",
    "indexer": "bun run source/services/indexer.ts",
    "dashboard": "bun run source/services/dashboard.ts",
    "oracle": "bun run source/services/oracle.ts",
    "lint": "eslint source/**/*.ts",
    "lint:fix": "eslint source/**/*.ts --fix",
    "format": "prettier --write source/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "ethers": "^6.8.1",
    "hardhat": "^2.19.4",
    "@openzeppelin/contracts": "^5.0.1",
    "@supabase/supabase-js": "^2.38.4",
    "ipfs-http-client": "^60.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "sequelize": "^6.35.1",
    "merkle-tree": "^1.0.2",
    "keccak256": "^1.0.6",
    "multihashes": "^0.4.21"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/uuid": "^9.0.7",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "hardhat-deploy": "^0.12.0",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.8.5"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "blockchain",
    "ai",
    "audit",
    "smart-contracts",
    "ethereum",
    "ipfs",
    "logging",
    "compliance"
  ],
  "author": "Chronicler Team",
  "license": "MIT"
} 